<% if @skills.present? %>
  <%= render 'filters' %>
  <%= render 'table' %>
  <div class="container">
    <div class="pagination_wrap">
      <%= paginate @skills %>
    </div>
  </div>
<% else %>
  <div class="m-t-40"/>
  <%= render 'table' %>
  <div class="text-center fw-bold error_msg"> No Skills Found! </div>
<% end %>