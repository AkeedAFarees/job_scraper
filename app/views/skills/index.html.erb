<% if @skills.present? %>
  <%= render 'filters' %>
  <%= render 'table' %>
  <div class="container">
    <div class="pagination_wrap">
      <%= paginate @skills %>
    </div>
  </div>
<% else %>
  <%= render 'table' %>
  <div class="text-center fw-bold no-skills"> No Skills Found! </div>
<% end %>