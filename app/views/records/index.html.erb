<%= render 'search_options' %>

<%- if @records.present? %>
  <%= render 'filters' %>
  <%= render 'table' %>
  <div class="container">
    <div class="pagination_wrap">
      <%= paginate @records %>
    </div>
  </div>

<% else %>
  <%= render 'filters' %>
  <div class="text-center fw-bold"> No Record Found! </div>
<% end %>
