  <td><%= record.company %></td>
  <td><%= link_to record.link.split("https://cv.ee/en/vacancy/").last.split("/", 2).last, record.link, target: :_blank%></td>
  <td><%= record.published_date %></td>
  <td><%= record.published_type %></td>
